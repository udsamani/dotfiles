            "args": [
                "-l"
            ]
        },
        "fish": {
            "path": "fish",
            "args": [
                "-l"
            ]
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        },
        "/opt/homebrew/bin/ny": {
            "path": "/bin/bash",
            "args": [
                "-l"
            ]
        },
        "nu": {
            "path": "/opt/homebrew/bin/nu",
            "args": [
